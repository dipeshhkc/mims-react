{
  "name": "react-start-mims",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.1.1",
    "@material-ui/icons": "^4.2.0",
    "axios": "^0.21.1",
    "jquery": "^3.4.1",
    "mui-datatables": "^2.5.1",
    "react": "^16.8.6",
    "react-data-export": "^0.5.0",
    "react-dom": "^16.8.6",
    "react-intl": "^2.9.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-select": "^3.0.4",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "smartmenus": "^1.1.0",
    "tempa-xlsx": "0.0.1"
  },
  "scripts": {
    "extract": "NODE_ENV=development babel ./src  --out-file /dev/null",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "babel-plugin-react-intl": "^3.3.3"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "react-intl",
        {
          "messagesDir": "./build/messages",
          "extractSourceLocation": true
        }
      ]
    ]
  }
}
